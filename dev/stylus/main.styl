		.slct
			width: 140px
			font-size: 15px
			text-decoration: none
			color: #fff
			&.active
				color: $main_txt_color
				a
					color: $main_txt_color
			a
				text-decoration: none
				color: #fff
		ul
			li
				margin-bottom: 10px
				&.current-lang
					a
						color: $main_color
						position: relative
						&:after
							content: ""
							width: 2px
							height: 100%
							display: block
							position: absolute
							left: 0px
							top: 0
							background-color: $main_color
				a
					display: block
					font-size: 15px
					text-decoration: none
					color: $main_txt_color
					transition: 0.5s
					&:hover
						color: $main_color
						text-decoration: none
					&:active
						text-decoration: none
					&:focus
						text-decoration: none
		.drop
			li
				padding: 0
				a
					display: block
