// aside
.aside
	padding-bottom: 20px
	&.fixed
		position: fixed
		top: 30px
.aside__items
	height: 100%
	overflow-y: auto
	overflow-x: hidden
	padding-top: 20px
	background-color: #f9f9f9
	//внешний вид скроллбара
	scrollbar-base-color: #CCCCCC  //для IE
	scrollbar-arrow-color: #CCCCCC 	//для IE
	//внешний вид скроллбара для -webkit- остальное тут https://poselab.com/en/blog/custom-scrollbars-in-webkit/
	&::-webkit-scrollbar
		width: 10px
	&::-webkit-scrollbar-thumb
		background-color: #CCCCCC
		&:hover
			background-color: darken(#CCCCCC  , 10%)
	&::-webkit-scrollbar-track-piece
		background-color: transparent
	ul
		li
			&.category
				position: relative
				a
					padding-top: 10px
					padding-bottom: 10px
					position: relative
					cursor: default
					font-weight: 800
					font-size: 20px
					&:after
						content: ""
						width: 50px
						height: 2px
						display: block
						position: absolute
						bottom: 0px
						left: 20px
						background-color: #000000
					&:hover
						background-color: transparent
						color: $main_txt_color
						text-decoration: none
					&:active
						background-color: transparent
						color: $main_txt_color
						text-decoration: none
					&:focus
						background-color: transparent
						color: $main_txt_color
						text-decoration: none
			&.aside-dropdown
				position: relative
				&>a
					padding-top: 10px
					padding-bottom: 10px
					position: relative
					font-weight: 800
					font-size: 20px
					&:after
						content: ""
						width: 50px
						height: 2px
						display: block
						position: absolute
						bottom: 0
						left: 20px
						background-color: #000000
						transition: bottom 0.3s
					&:hover
						background-color: transparent
						text-decoration: none
					&:active
						background-color: transparent
						text-decoration: none
					&:focus
						background-color: transparent
						text-decoration: none

				i
					color: $main_txt_color
					position: absolute
					top: 11px
					left: 1px
					transform rotate(0deg)
					z-index: 10
					transition: 0.3s
				&.m-open
					i
						transform rotate(90deg)
			&.current_page_item
				a
					background-color: lightness($main_color,80%)
					color: $main_txt_color
					position: relative
					&:hover
						background-color: lightness($main_color,80%)
						text-decoration: none
					&:after
						content: ""
						width: 2px
						height: 100%
						display: block
						position: absolute
						left: 1px
						top: 0
						background-color: $main_color
			&.current-menu-item
				a
					background-color: lightness($main_color,80%)
					color: $main_txt_color
					position: relative
					&:hover
						background-color: lightness($main_color,80%)
						text-decoration: none
					&:after
						content: ""
						width: 2px
						height: 100%
						display: block
						position: absolute
						left: 1px
						top: 0
						background-color: $main_color
			a
				display: block
				padding-top: 7px
				padding-bottom: 7px
				padding-left: 20px
				font-size: 15px
				background-color: transparent
				color: $main_txt_color
				text-decoration: none
				transition: 0.3s
				&:hover
					background-color: #ccc
					text-decoration: none
				&:active
					background-color: #ccc
					text-decoration: none
				&:focus
					background-color: #ccc
					text-decoration: none
				&.navmenu-link-toggle
					display: block
					&:hover
						background-color: transparent
						text-decoration: none
					&:active
						text-decoration: none
					&:hover:after
						bottom: 3px
			.aside__submenu
				display: none
				padding-left: 0
				padding-top: 5px
				li
					&:last-of-type
						padding-bottom: 0
// Пример checkbox
@keyframes checkColor
	from
		color: #727272
	to
		color: $main_color
@keyframes checkColorBack
	from
		color: $main_color
	to
		color: #000
label.check, label.radio
	display: inline-flex
	align-items: center
	color: #000
	cursor: pointer
	font-size: 14px
	user-select: none
	i
		width: 11px
		display: inline-flex
		margin-right: 10px
		font-size: 14px
		animation: checkColorBack
		animation-duration: 0.7s
		animation-direction alternate
		animation-fill-mode forwards
	span
		display: inline-block
		font-size: 14px
	input[type=checkbox],
	input[type=radio]
		display: none
		&:not(:checked)  ~ .fa-2
			display: none
		&:checked ~ i
			&.fa-1
				display: none
			&.fa-2
				display: inline-flex
				animation: checkColor
				animation-duration: 0.7s
				animation-direction alternate
				animation-fill-mode forwards

