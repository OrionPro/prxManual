// внешний вид табов и анимация
.tabs-all-items-wrap
	margin-bottom: 30px
.tabs-items-wrap
	margin-bottom: 10px
.tabs-item
	&.active
		background-color: #fff
		border: 1px solid $main_color
		color: $main_color
.tabs-wrap
	display: none
	padding: 20px
	background-color: rgba(255, 255, 255,0.5)
	&.active
		display: block
		animation: fade-bot
		animation-duration: 0.7s

.tabs-wrap-inner
	display: none
	padding: 20px
	&.active
		display: block
		animation: fade
		animation-delay: 0.3s
		animation-fill-mode: backwards
		animation-duration: 0.7s!important // почему то при build стили объединяются с .tabs-wrap и не работает animation-duration, приходится !important применять
@keyframes fade
	from
		opacity: 0
	to
		opacity: 1
@keyframes fade-bot
	from
		opacity: 0
		transform: translateY(20px)
	to
		opacity: 1
		transform: translateY(0)

